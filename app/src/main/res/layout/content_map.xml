<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MapActivity"
    tools:layout_editor_absoluteY="25dp"
    tools:showIn="@layout/app_bar_map">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="384dp"
        android:layout_height="567dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/samplemap" />

    <ImageButton
        android:id="@+id/floors_button"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="86dp"
        android:background="@drawable/button_round"
        android:contentDescription="@string/todo"
        android:src="@drawable/ic_layers_black_24dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/tools_button"
        app:layout_constraintStart_toStartOf="@+id/tools_button"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/patient_assistant_button"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginBottom="11dp"
        android:layout_weight="1"
        android:clickable="true"
        android:focusable="true"
        app:backgroundTint="@color/colorPrimaryLight"
        app:layout_constraintBottom_toTopOf="@+id/food_button"
        app:layout_constraintEnd_toEndOf="@+id/food_button"
        app:layout_constraintStart_toStartOf="@+id/food_button"
        app:layout_constraintTop_toBottomOf="@+id/floors_button"
        app:srcCompat="@drawable/ic_accessibility_black_24dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/food_button"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginBottom="12dp"
        android:layout_weight="1"
        android:clickable="true"
        android:focusable="true"
        app:backgroundTint="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/wc_button"
        app:layout_constraintEnd_toEndOf="@+id/wc_button"
        app:layout_constraintStart_toStartOf="@+id/wc_button"
        app:layout_constraintTop_toBottomOf="@+id/patient_assistant_button"
        app:srcCompat="@drawable/ic_restaurant_black_24dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/wc_button"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginBottom="8dp"
        android:layout_weight="1"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintBottom_toTopOf="@+id/tools_button"
        app:layout_constraintEnd_toEndOf="@+id/tools_button"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tools_button"
        app:layout_constraintTop_toBottomOf="@+id/food_button"
        app:srcCompat="@drawable/ic_wc_black_24dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/tools_button"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        android:layout_weight="1"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_widgets_black_24dp" />

</android.support.constraint.ConstraintLayout>